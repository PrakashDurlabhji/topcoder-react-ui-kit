    "type": "git",
    "url": "git+https://github.com/topcoder-platform/topcoder-react-ui-kit.git"
  },
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/webpack --env=production --progress --profile --colors --display-optimization-bailout",
    "build:dev": "npm run clean && webpack --env=development --progress --profile --colors --display-optimization-bailout --bail --watch",
    "clean": "rimraf dist",
    "jest": "jest --no-cache --maxWorkers=4 --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.scss --syntax scss",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run jest"
  },
  "version": "0.2.5",
  "dependencies": {
    "react": "^16.3.2",
    "react-css-super-themr": "^2.2.0",
